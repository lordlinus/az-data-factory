{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Blob_Synapse_Copy_Activity",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "*"
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"allowPolyBase": true,
						"polyBaseSettings": {
							"rejectValue": 0,
							"rejectType": "value",
							"useTypeDefault": true
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DataSet_01_Blob_Excel_Files",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSynapseAnalyticsTable",
						"type": "DatasetReference",
						"parameters": {
							"tableName": "tbl-dataset-01"
						}
					}
				]
			}
		],
		"folder": {
			"name": "Pipeline-01"
		},
		"annotations": []
	}
}